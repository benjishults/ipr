(implies (and (forall ((x)) (not (less-than x x)))
              (forall ((x1) (x2) (x3))
                (implies (and (less-than x1 x2)
                              (less-than x2 x3))
                  (less-than x1 x3))))
  (forall ((x) (y))
    (or (less-than x y)
        (less-than y x)
        (not (for-some ((z)) (and (less-than z x)
                                  (less-than y z)))))))
