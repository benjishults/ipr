      (and (forall ((x)) (implies (food x) (likes john x)))
           (food apples)
           (food chicken)
           (forall ((x)) (implies
                             (for-some ((y)) (and (eats y x)
                                                  (not-killed-by y x) ))
                           (food x)))
           (eats bill peanuts)
           (alive bill)
           (forall ((x)) (implies (eats bill x) (eats sue x)))
           (forall ((y)) (implies (alive y)
                           (forall ((x)) (not-killed-by y x)))))
    (likes john peanuts)))

